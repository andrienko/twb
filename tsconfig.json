{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ES6"
    ],
    "module": "ESNext",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "declaration": false
  }
}
